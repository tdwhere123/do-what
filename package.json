{
  "name": "@do-what/workspace",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "pnpm run dev:business",
    "dev:desktop": "pnpm --filter @do-what/desktop dev",
    "dev:lite": "echo \"[deprecated] dev:lite -> dev:business\" && pnpm run dev:business",
    "dev:ui": "pnpm --filter @do-what/ui dev",
    "clean:artifacts": "node scripts/clean-artifacts.mjs",
    "build": "node scripts/build.mjs",
    "build:ui": "pnpm --filter @do-what/ui build",
    "doctor:windows": "powershell -ExecutionPolicy Bypass -File scripts/setup/windows/doctor.ps1",
    "setup:windows": "powershell -ExecutionPolicy Bypass -File scripts/setup/windows/install.ps1",
    "bootstrap:windows": "powershell -ExecutionPolicy Bypass -File scripts/setup/windows/bootstrap.ps1",
    "typecheck": "pnpm --filter @do-what/ui typecheck",
    "test:health": "pnpm --filter @do-what/ui test:health",
    "test:sessions": "pnpm --filter @do-what/ui test:sessions",
    "test:refactor": "pnpm --filter @do-what/ui test:refactor",
    "test:events": "pnpm --filter @do-what/ui test:events",
    "test:todos": "pnpm --filter @do-what/ui test:todos",
    "test:permissions": "pnpm --filter @do-what/ui test:permissions",
    "test:session-switch": "pnpm --filter @do-what/ui test:session-switch",
    "test:fs-engine": "pnpm --filter @do-what/ui test:fs-engine",
    "test:e2e": "pnpm --filter @do-what/ui test:e2e",
    "test:orchestrator": "pnpm --filter @do-what/orchestrator typecheck",
    "bump:patch": "pnpm --filter @do-what/ui bump:patch",
    "bump:minor": "pnpm --filter @do-what/ui bump:minor",
    "bump:major": "pnpm --filter @do-what/ui bump:major",
    "bump:set": "pnpm --filter @do-what/ui bump:set",
    "tauri": "pnpm --filter @do-what/desktop exec tauri",
    "dev:business": "pnpm --filter @do-what/ui dev"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@whiskeysockets/baileys",
      "better-sqlite3",
      "esbuild",
      "protobufjs"
    ],
    "patchedDependencies": {
      "@solidjs/router@0.15.4": "patches/@solidjs__router@0.15.4.patch"
    }
  },
  "packageManager": "pnpm@10.27.0"
}
